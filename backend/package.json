  "description": "Backend server for MutationMechanic project with Prisma and PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec tsx src/index.ts",
